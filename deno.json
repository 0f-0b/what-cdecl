{
  "compilerOptions": {
    "noImplicitOverride": true,
    "useUnknownInCatchVariables": true
  },
  "importMap": "import_map.json",
  "exclude": [
    "dist"
  ],
  "tasks": {
    "lint": "deno fmt --check && deno lint && deno check --all scripts/build.tsx",

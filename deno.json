{
  "imports": {
    "@deno/esbuild-plugin": "jsr:@deno/esbuild-plugin@^1.2.0",
    "@std/fs": "jsr:@std/fs@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.0",
    "esbuild": "npm:esbuild@0.25.10",
    "hast-util-to-html": "npm:hast-util-to-html@9.0.5",
    "hastscript": "npm:hastscript@9.0.1"
  },
  "exclude": [
    "static"
  ],
  "lint": {
    "rules": {
      "include": [
        "verbatim-module-syntax"
      ]
    }
  },
  "tasks": {
    "cache": "deno cache --allow-import=jsr.io:443 scripts/build.tsx",
    "lint": "deno fmt --check && deno lint && deno check --all --allow-import=jsr.io:443 scripts/build.tsx"
  }
}
